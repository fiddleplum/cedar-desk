{
	"name": "@fiddleplum/cedar-desk",
	"version": "0.0.0",
	"description": "Cedar Desk",
	"private": true,
	"scripts": {
		"setup": "yarn install --pure-lockfile; mkdir -p dist; cp src/config.example.js dist/config.js",
		"update": "yarn install",
		"build": "webpack --config webpack.dev.js",
		"build-production": "webpack --config webpack.prod.js",
		"start": "webpack-dev-server --config webpack.dev.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fiddleplum/cedar-desk.git"
	},
	"author": "Stephen O'Rourley",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/fiddleplum/cedar-desk/issues"
	},
	"homepage": "https://github.com/fiddleplum/cedar-desk",
	"devDependencies": {
		"eslint": "latest",
		"@typescript-eslint/parser": "latest",
		"@typescript-eslint/eslint-plugin": "latest",
		"raw-loader": "latest",
		"ts-loader": "latest",
		"tsconfig-paths-webpack-plugin": "latest",
		"typescript": "latest",
		"webpack": "latest",
		"webpack-cli": "latest",
		"webpack-dev-server": "latest",
		"webpack-merge": "latest",
		"copy-webpack-plugin": "latest"
	}
}
